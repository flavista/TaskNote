<!DOCTYPE html>
<html lang="es">
<head>

    <meta charset=utf-8 />
    <title>TaskNote</title>
    <!-- este le da el formato de navbar-->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
          integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    <link href="@Url.Content("~/css/Overviews.css")" rel="stylesheet" type="text/css" />
    @RenderSection("Styles", false)

</head>
<body>

    @RenderBody()

    <!--lo agregue de la web de bootstrap porque sino no me andaba el dropdown del navbar https://getbootstrap.com/docs/4.3/getting-started/download/ -->
